{
  "name": "greet-mertens-website",
  "version": "1.0.1",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "build:lambda": "netlify-lambda build src/functions",
    "code:check:rules": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "code:check": "yarn code:lint; yarn code:format --check",
    "code:clean": "yarn code:lint --fix; yarn code:format --write",
    "code:format": "prettier './src/**/**.{js,jsx,ts,tsx}'",
    "code:lint": "eslint --ext .js,.jsx,.ts,.tsx \"src/\"",
    "dev": "gatsby develop"
  },
  "dependencies": {
    "@sanity/block-content-to-react": "^2.0.7",
    "@sanity/image-url": "^0.140.22",
    "@sindresorhus/class-names": "^1.1.0",
    "cross-fetch": "^3.0.6",
    "date-fns": "^2.18.0",
    "dotenv": "^8.2.0",
    "flickity": "^2.2.2",
    "gatsby": "^3.0.0",
    "gatsby-image": "^2.4.5",
    "gatsby-plugin-graphql-codegen": "^2.7.1",
    "gatsby-plugin-layout": "^2.0.0",
    "gatsby-plugin-manifest": "^3.0.0",
    "gatsby-plugin-next-seo": "^1.7.0",
    "gatsby-plugin-postcss": "^4.0.0",
    "gatsby-plugin-root-import": "^2.0.6",
    "gatsby-plugin-sharp": "^3.0.0",
    "gatsby-source-sanity": "^6.0.5",
    "gatsby-transformer-sharp": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-flickity-component": "^3.6.1",
    "react-helmet-async": "^1.0.8",
    "react-player": "^2.9.0",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.13.0",
    "@types/aws-lambda": "^8.10.72",
    "@types/flickity": "^2.2.4",
    "@types/yup": "^0.29.11",
    "autoprefixer": "^10.2.4",
    "babel-preset-gatsby": "^1.0.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "netlify-lambda": "^2.0.3",
    "poetic": "^1.6.0",
    "postcss": "^8.2.6",
    "tailwindcss": "^2.0.3"
  }
}
