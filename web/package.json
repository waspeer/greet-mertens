{
  "name": "greet-mertens-website",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "code:check:rules": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "code:check": "yarn code:lint; yarn code:format --check",
    "code:clean": "yarn code:lint --fix; yarn code:format --write",
    "code:format": "prettier './src/**/**.{js,jsx,ts,tsx}'",
    "code:lint": "eslint --ext .js,.jsx,.ts,.tsx \"src/\"",
    "dev": "gatsby develop"
  },
  "dependencies": {
    "@sanity/block-content-to-react": "^2.0.7",
    "@sanity/image-url": "^0.140.19",
    "@sindresorhus/class-names": "^1.1.0",
    "date-fns": "^2.14.0",
    "dotenv": "^8.2.0",
    "gatsby": "^2.21.33",
    "gatsby-image": "^2.4.5",
    "gatsby-plugin-graphql-codegen": "^2.7.1",
    "gatsby-plugin-layout": "^1.3.1",
    "gatsby-plugin-manifest": "^2.4.18",
    "gatsby-plugin-next-seo": "^1.6.0",
    "gatsby-plugin-postcss": "^2.3.1",
    "gatsby-plugin-root-import": "^2.0.5",
    "gatsby-plugin-sharp": "^2.6.3",
    "gatsby-source-sanity": "^5.0.6",
    "gatsby-transformer-sharp": "^2.5.2",
    "postcss-preset-env": "^6.7.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet-async": "^1.0.6",
    "react-player": "^2.3.1",
    "tailwindcss": "^1.4.6"
  },
  "devDependencies": {
    "eslint-import-resolver-typescript": "^2.0.0",
    "poetic": "^1.5.0"
  }
}
